// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Mensagem {
  id        String   @id @default(uuid())
  titulo    String
  conteudo  String
  publicado Boolean   
  qtdeLikes Int 
  @@map("mensagem")
}
